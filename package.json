{
   "name": "nodejs-graphql-typeorm-ts",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "devDependencies": {
      "@types/node": "^14.14.31",
      "@types/uuid": "^8.3.0",
      "@types/uuidv4": "^5.0.0",
      "@typescript-eslint/eslint-plugin": "^4.16.1",
      "@typescript-eslint/parser": "^4.16.1",
      "eslint": "^7.21.0",
      "eslint-config-google": "^0.14.0",
      "nodemon": "^2.0.7",
      "ts-node": "9.1.1",
      "typescript": "4.1.5"
   },
   "dependencies": {
      "@graphql-tools/graphql-file-loader": "^6.2.7",
      "@graphql-tools/load": "^6.2.7",
      "@graphql-tools/utils": "^7.5.0",
      "@types/bcryptjs": "^2.4.2",
      "@types/jest": "^26.0.20",
      "bcryptjs": "^2.4.3",
      "gql2ts": "^2.0.0-0",
      "graphql-import": "^1.0.2",
      "graphql-yoga": "^1.18.3",
      "jest": "^26.6.3",
      "pg": "^8.5.1",
      "reflect-metadata": "^0.1.13",
      "ts-jest": "^26.5.3",
      "typeorm": "0.2.31",
      "uuid": "^8.3.2",
      "uuidv4": "^6.2.6"
   },
   "scripts": {
      "start": "nodemon --exec ts-node --files src/index.ts",
      "dev": "nodemon --exec ts-node --files src/index.ts",
      "gen-schema-types": "gql2ts src/schema.graphql -o src/types/schema.d.ts"
   },
   "jest": {
      "transform": {
         "^.+\\.tsx?$": "ts-jest"
      },
      "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
      "moduleFileExtensions": [
         "ts",
         "tsx",
         "js",
         "jsx",
         "json",
         "node"
      ]
   }
}